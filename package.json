{
  "name": "8004-solana-indexer",
  "version": "1.3.0",
  "description": "Custom Solana indexer for 8004 Agent Registry v0.4.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:localnet": "cp .env.localnet .env && tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:tsx": "npx tsx src/index.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:localnet": "vitest run --config vitest.e2e.config.ts -t 'Localnet'",
    "test:all": "vitest run --coverage && vitest run --config vitest.e2e.config.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "solana",
    "indexer",
    "8004",
    "agent-registry",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@prisma/client": "^6.19.2",
    "@solana/web3.js": "^1.98.0",
    "@supabase/supabase-js": "^2.49.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "8004-solana": "github:QuantuLabs/8004-solana-ts#v0.6.0",
    "bs58": "^6.0.0",
    "cors": "^2.8.6",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "isomorphic-dompurify": "^2.35.0",
    "lru-cache": "^11.2.5",
    "p-queue": "^9.1.0",
    "pg": "^8.16.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "@mongodb-js/zstd": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.6",
    "@vitest/coverage-v8": "^2.1.9",
    "prisma": "^6.19.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
